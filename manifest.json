{
  "manifest_version": 2,
  "name": "Your Notes",
  "description": "Keep track your notes",
  "version": "1.0",
  "browser_action": {
    "default_icon": "./icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["popup.js"]
    }
  ],

  "permissions": [
    "storage",
    "activeTab",
     "http://ajax.googleapis.com/"]
}
